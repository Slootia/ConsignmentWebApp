
@{
    Layout = null;
    string tempName = string.Empty;
}

<!DOCTYPE html>

@foreach (var c in ViewBag.Consignments)
{
    if (c.Id == ViewBag.Id)
    {
        tempName = c.Name;
    }
}
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>О партии @tempName</title>
</head>
<body>
    <div>

            <h3>О партии @tempName</h3>
        </div>
</body>
</html>
